<ScrollView android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/tools">

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:background="@drawable/app_background"
        xmlns:fresco="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="#212121"
            android:orientation="vertical" />


        <com.facebook.drawee.view.SimpleDraweeView
            android:id="@+id/layout_image"
            android:layout_width="match_parent"
            android:layout_height="270dp"
            />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="#212121"
            android:orientation="vertical" />


        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/myCardView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            card_view:cardBackgroundColor="@android:color/transparent"
            card_view:cardElevation="8dp">

            <com.avinash_ksworks.jci.nammabengaluru.headingText
                    android:padding="10dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/place_textView"
                    android:layout_width="wrap_content"
                    android:textAllCaps="true"
                    android:textSize="26sp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="#ffffff"
                    />
        </android.support.v7.widget.CardView>




        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >


            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                card_view:cardBackgroundColor="@android:color/transparent"
                card_view:cardElevation="8dp">
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >
                            <com.airbnb.lottie.LottieAnimationView
                                android:id="@+id/gallery_animation"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                />
                            <com.avinash_ksworks.jci.nammabengaluru.readingText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#ffffff"
                                android:text="GALLERY"
                                android:layout_below="@+id/gallery_animation"
                                android:layout_centerHorizontal="true"
                                />
                        </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                card_view:cardBackgroundColor="@android:color/transparent"
                card_view:cardElevation="8dp">
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >
                            <com.airbnb.lottie.LottieAnimationView
                                android:id="@+id/favourite_animation"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                />

                            <com.avinash_ksworks.jci.nammabengaluru.readingText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#ffffff"
                                android:text="FAVOURITE"
                                android:layout_below="@+id/favourite_animation"
                                android:layout_centerHorizontal="true"
                                />

                            </RelativeLayout>
            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                card_view:cardBackgroundColor="@android:color/transparent"
                card_view:cardElevation="8dp">
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >

                            <com.airbnb.lottie.LottieAnimationView
                                android:id="@+id/visited_animation"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                />

                            <com.avinash_ksworks.jci.nammabengaluru.readingText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#ffffff"
                                android:text="VISITED"
                                android:layout_below="@+id/visited_animation"
                                android:layout_centerHorizontal="true"
                                />
                        </RelativeLayout>

            </android.support.v7.widget.CardView>


            <android.support.v7.widget.CardView
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                card_view:cardBackgroundColor="@android:color/transparent"
                card_view:cardElevation="8dp">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <com.airbnb.lottie.LottieAnimationView
                        android:id="@+id/calling_animation"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        />

                    <com.avinash_ksworks.jci.nammabengaluru.readingText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#ffffff"
                        android:text="CONTACT"
                        android:layout_below="@+id/calling_animation"
                        android:layout_centerHorizontal="true"
                        />
                </RelativeLayout>

            </android.support.v7.widget.CardView>
        </LinearLayout>




        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            app:cardBackgroundColor="@color/colorWhite"
            app:cardElevation="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.avinash_ksworks.jci.nammabengaluru.headingText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="Description"
                    android:textColor="@color/defTextColor"
                    android:textAllCaps="true"
                    android:textSize="18sp" />

                <View style="@style/Divider" />

                <com.avinash_ksworks.jci.nammabengaluru.textFont
                    android:id="@+id/description_textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:layout_margin="8dp"
                    />
            </LinearLayout>
        </android.support.v7.widget.CardView>


        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            app:cardBackgroundColor="@color/colorWhite"
            app:cardElevation="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.avinash_ksworks.jci.nammabengaluru.headingText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:textAllCaps="true"
                    android:text="Best Season"
                    android:textColor="@color/defTextColor"
                    android:textSize="18sp" />

                <View style="@style/Divider" />

                <com.avinash_ksworks.jci.nammabengaluru.textFont
                    android:id="@+id/season_textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:layout_margin="8dp"
                    />

            </LinearLayout>
        </android.support.v7.widget.CardView>



        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            app:cardBackgroundColor="@color/colorWhite"
            app:cardElevation="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.avinash_ksworks.jci.nammabengaluru.headingText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="Entry Fee"
                    android:textColor="@color/defTextColor"
                    android:textAllCaps="true"
                    android:textSize="18sp" />

                <View style="@style/Divider" />

                <com.avinash_ksworks.jci.nammabengaluru.textFont
                    android:id="@+id/entryFee_textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:layout_margin="8dp"
                    />
            </LinearLayout>
        </android.support.v7.widget.CardView>


        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            app:cardBackgroundColor="@color/colorWhite"
            app:cardElevation="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.avinash_ksworks.jci.nammabengaluru.headingText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:textAllCaps="true"
                    android:text="Additional Information"
                    android:textColor="@color/defTextColor"
                    android:textSize="18sp" />

                <View style="@style/Divider" />

                <com.avinash_ksworks.jci.nammabengaluru.textFont
                    android:id="@+id/additionalInformation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:layout_margin="8dp"
                     />

            </LinearLayout>
        </android.support.v7.widget.CardView>



        <Button
            android:id="@+id/gmapButton"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="15dp"
            android:background="@drawable/rounded_button"
            android:text="View on maps"
            android:textColor="@color/colorWhite" />

        <android.support.v4.widget.Space
            android:layout_width="match_parent"
            android:layout_height="40dp" />




    </LinearLayout>

</ScrollView>
